{
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "editor.formatOnType": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "files.associations": {
    "*.js": "javascriptreact"
  },
  "search.useIgnoreFiles": true,
  "files.exclude": {
    "**/node_modules": true
  },
  "cSpell.words": [
    "autodocs",
    "autofix",
    "codegen",
    "disp",
    "entypo",
    "hookform",
    "ibjapan",
    "ibjreserve",
    "Iconify",
    "mypage",
    "tanstack",
    "telno",
    "USERLIST",
    "zustand"
  ],
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#ffa51e",
    "activityBar.activeBorder": "#ffa51e",
    "activityBar.background": "#ffa51e",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#ffffff",
    "activityBarBadge.foreground": "#000000",
    "statusBar.background": "#ffa51e",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#ffa51e",
    "titleBar.activeBackground": "#ffa51e",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#ffa51e",
    "titleBar.inactiveForeground": "#15202b99",
    "commandCenter.border": "#15202b99",
    "sash.hoverBorder": "#ffa51e",
    "statusBarItem.remoteBackground": "#ffa51e",
    "statusBarItem.remoteForeground": "#15202b"
  }
}
